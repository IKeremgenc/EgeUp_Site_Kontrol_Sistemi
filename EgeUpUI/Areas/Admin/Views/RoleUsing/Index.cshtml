@model List<DataaccsessLayer.Table.AppUser>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Admin_Layout.cshtml";
}

<nav aria-label="breadcrumb">
    <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="/Admin/HomePageAdmin/Index/">Ana Sayfa</a></li>
        <li class="breadcrumb-item"><a href="/Admin/RoleUsing/Index"> Rol Listesi</a></li>

    </ol>
</nav>


<div class="row">
    <div class="col-12">
        <div class="card">
            <div class="card-header">
                <h3 class="card-title">Rolle Listesi</h3>

            </div>

            <div class="card-body table-responsive p-0">
                <table class="table table-hover text-nowrap">
                    <thead>
                        <tr>
                            <th>Kullanıcılar</th>
                         
                            <th>Rol Ata</th>


                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {


                            <tr>

                                <td>@item.Name</td>
                               
                                <td><a href="/Admin/RoleUsing/UssingnRole/@item.Id" class="btn btn-outline-primary">Rol Ata</a></td>
                              

                            </tr>
                        }
                    </tbody>
                </table>
                <br />
                <br />

            </div>
            <!-- /.card-body -->
        </div>
        <!-- /.card -->
    </div>
</div>


