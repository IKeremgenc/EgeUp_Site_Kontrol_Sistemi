<script src="https://cdn.jsdelivr.net/npm/sweetalert2@10"></script>







<script>
    $(document).ready(function () {
        $('.SilWebsite').click(function () {
            var id = $(this).data('id');

            Swal.fire({
                title: 'İptal Edilecek!',
                text: "İptal etmek istediğinizden emin misiniz!",
                icon: 'warning',
                showCancelButton: true,
                confirmButtonColor: '#3085d6',
                cancelButtonColor: '#d33',
                confirmButtonText: 'Evet!',
                cancelButtonText: 'İptal'
            }).then((result) => {
                if (result.isConfirmed) {
                    const url = '/WebList/Delet/' + id;
                    $.ajax({
                        type: "GET",
                        url: url,
                        success: function (data) {
                            Swal.fire(
                                'İptal Edildi!',
                                'Seçili toplantı iptal edildi..',
                                'success'
                            );
                            timer: 3500
                            location.reload();
                        },
                        error: function (err) {
                            toastr.error("Bir hata oluştu! " + `${err.statusText}`);
                        }
                    });
                }
            });
        });
    });
</script>

